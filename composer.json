{
    "name": "aaronfrancis/flaky",
    "description": "A Laravel package to elegantly handle flaky operations.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Francis",
            "email": "aaron@tryhardstudios.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.5|^11.0",
        "orchestra/testbench": "^8.21|^9.1|^10.0"
    },
    "autoload": {
        "psr-4": {
            "AaronFrancis\\Flaky\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AaronFrancis\\Flaky\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AaronFrancis\\Flaky\\Providers\\FlakyServiceProvider"
            ]
        }
    }
}
